{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist/backend", //ここかえたほうがよさそう
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["backend/**/*"], //ここかえたほうがよさそう
  "exclude": ["node_modules"]
}
